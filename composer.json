        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "doctrine/annotations": "^1.6",
        "fig/http-message-util": "^1.1",
        "moneyphp/money": "^3.1",
        "ocramius/proxy-manager": "^2.2",
        "php-di/php-di": "^6.0",
        "psr/container": "^1.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
